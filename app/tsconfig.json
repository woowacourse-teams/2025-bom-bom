{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "plugins/withCustomAndroidConfig.js",
    "app.config.js",
    "plugins/withNetworkSecurityConfigFile.js"
  ]
}

{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "web:start": "pnpm run start --workspace=web",
    "web:build": "pnpm run build --workspace=web",
    "web:lint": "pnpm run lint --workspace=web",
    "web:test": "pnpm run test --workspace=web",
    "app:start": "pnpm run start --workspace=app",
    "app:android": "pnpm run android --workspace=app",
    "app:ios": "pnpm run ios --workspace=app",
    "shared:build": "pnpm run build --workspace=shared",
    "type-check": "pnpm run type-check",
    "lint": "pnpm run lint",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  }
}
